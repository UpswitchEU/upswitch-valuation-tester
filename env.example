# Valuation Engine API Configuration
# Primary API URL for valuation engine
VITE_VALUATION_ENGINE_URL=https://upswitch-valuation-engine-production.up.railway.app

# Alternative API URL (fallback)
VITE_VALUATION_API_URL=https://upswitch-valuation-engine-production.up.railway.app

# Backend URL for saving valuations (Node.js backend)
VITE_BACKEND_URL=https://api.upswitch.biz
VITE_API_BASE_URL=https://api.upswitch.biz

# Feature Flags
VITE_ENABLE_REGISTRY=true
VITE_ENABLE_DOCUMENT_UPLOAD=true
VITE_ENABLE_AI_CONVERSATION=true

# Development Settings
VITE_DEBUG=false
VITE_NODE_ENV=production

# Local Development (uncomment for local development)
# VITE_VALUATION_ENGINE_URL=http://localhost:8000
# VITE_DEBUG=true
# VITE_NODE_ENV=development
