{
  "name": "upswitch-valuation-tester",
  "private": true,
  "version": "1.0.0",
  "description": "Delphi - UpSwitch Valuation Tester Interface - The Oracle's wisdom for business insights",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3001",
    "build": "tsc && vite build",
    "vercel-build": "NODE_ENV=production npm run build",
    "build:production": "tsc && vite build --config vite.config.production.ts",
    "preview": "vite preview --port 3001",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "deploy": "./deploy.sh",
    "deploy:vercel": "./deploy.sh vercel",
    "deploy:netlify": "./deploy.sh netlify",
    "deploy:aws": "./deploy.sh aws",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@heroui/react": "^2.8.3",
    "@heroui/theme": "^2.4.21",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dompurify": "^3.3.0",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "html2pdf.js": "^0.12.1",
    "lucide-react": "^0.400.0",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.9.3",
    "recharts": "^2.15.3",
    "tailwindcss": "^3.4.17",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/eventsource": "^1.1.15",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.35",
    "typescript": "^5.2.2",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "valuation",
    "business",
    "M&A",
    "testing",
    "react",
    "typescript"
  ],
  "author": "Upswitch Team",
  "license": "UNLICENSED"
}
